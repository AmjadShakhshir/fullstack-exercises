{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/dist/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1",
      "continue": true
    }
  ]
}
